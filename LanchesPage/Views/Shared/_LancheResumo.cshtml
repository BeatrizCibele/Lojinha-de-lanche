@model Lanche

<div class="col-sm-4">

    <div class="col">
        <img src="@Model.ImagemURL" class="img-thumbnail" alt="" />


        <div class="figure-caption">
            <h3 class="text-md-right" Preco: @Model.Preco.ToString("c")></h3>
            <h3>Lanche: @Model.Nome</h3>
            <p>@Model.DescricaoCurta</p>
        </div>
    </div>

</div>